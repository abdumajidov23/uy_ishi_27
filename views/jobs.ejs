<!DOCTYPE html>
<html lang="en">
    <%- include("./partials/head.ejs") %>
<body>
    <h1>Jobs</h1>
    <%- include("./partials/nav.ejs") %>

    <ul>
        <% jobs.forEach((job) => { %>
            <li>
                <h2><%= job.title %></h2>
                <p>Location: <%= job.location %></p>
                <p>Salary: <%= job.salary %></p>
            </li>
        <% }); %>
    </ul>
    
</body>
</html>